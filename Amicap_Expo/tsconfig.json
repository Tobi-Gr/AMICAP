{
  "extends": "expo/tsconfig.base",
  "esModuleInterop": "true", 
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "assets": ["./assets/fonts/"]
}
